<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.ssg.springex.mapper.TimeMapper2">
    <select id="getNow" resultType="string"> <!--id 값과 TimeMapper2.java에 선언해놓은 메소드 명이 같아야됨!!, resultType =반환 타입 -->
        select now()
    </select>
</mapper>



